package modelo;

import java.util.ArrayList;

public abstract class Aerolinea {
	private double impuestoPasajes;
	private ArrayList<AsientoReservado> asientosReservados = new ArrayList<AsientoReservado>();
	abstract void reservar(Usuario usuario,Asiento asiento);
	abstract void comprar(Usuario usuario,Asiento asiento);
	abstract void asientosDisponibles();
	
}
